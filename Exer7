package lpa2;

import java.util.Scanner;

public class Pekka {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		
		int soma=0;
		int n = 1;
		int idade=0;
		int maior=0;
		
		while (n <=20) {
			System.out.println("Informe 20 idades");
			idade = ler.nextInt();
			n++;
		   if (idade>17) {
			   maior++;
		   }
			
		}
		System.out.println("A quantidade de pessoas maior de idade s√£o: " + maior);
	}

}
