package lpa2;
import java.util.Scanner;
public class Titanic {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
	
		int soma=0;
		int n = 1;
		int idade=0;
		
		
		while (n <=20) {
			System.out.println("Informe 20 idades");
			idade = ler.nextInt();
			n++;
			soma=soma+idade;
		}
		System.out.println("A soma das idades das pessoas Ã©: " + soma);
	}

}
