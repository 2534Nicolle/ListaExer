package lpa2;
import java.util.Scanner;
public class AtleticoGOINIENSE {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
	
		int soma=0;
		int n = 0;
		
		while (n <=14) {
			n++;
			soma=soma+n;
		}
		System.out.println("A soma dos números de 1 a 15 é: " + soma);
	}

}
